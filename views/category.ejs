<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Menu</title>
	</head>
	<body>
		<%- include('./partials/nav') %>

		<h1><%= category %></h1>
		<% menuItems.forEach(item => { %>
		<div>
			<p>Name: <%= item.name %></p>
			<p>Price: $<%= item.price.toFixed(2) %></p>
			<p>Rating: <%= item.rating %> stars</p>
			<p>Details: <%= item.details %></p>
		</div>
		<% }) %>
	</body>
</html>
